{
	"name": "@brawltracker/cdn",
	"version": "1.3.0",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"lint": "eslint ."
	},
	"devDependencies": {
		"eslint": "^8.57.0",
		"tsup": "^8.2.4",
		"typescript": "5.8.2"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./v2": {
			"import": {
				"types": "./dist/v2.d.ts",
				"default": "./dist/v2.js"
			},
			"require": {
				"types": "./dist/v2.d.cts",
				"default": "./dist/v2.cjs"
			}
		}
	},
	"dependencies": {
		"ofetch": "^1.4.1"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/technorav3nn/brawl-tracker.git",
		"directory": "packages/cdn"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
