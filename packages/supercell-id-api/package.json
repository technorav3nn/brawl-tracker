{
	"name": "@brawltracker/supercell-id-api",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"lint": "eslint ."
	},
	"dependencies": {
		"ofetch": "^1.4.1"
	},
	"devDependencies": {
		"esbuild-plugins-node-modules-polyfill": "^1.6.8",
		"eslint": "^8.57.0",
		"tsup": "^8.2.4",
		"typescript": "5.7.3"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.js"
			}
		},
		"./browser": {
			"import": {
				"types": "./dist/browser.d.ts",
				"default": "./dist/browser.js"
			},
			"require": {
				"types": "./dist/browser.d.cts",
				"default": "./dist/browser.cjs"
			}
		},
		"./crypto": {
			"import": {
				"types": "./dist/crypto.d.ts",
				"default": "./dist/crypto.js"
			},
			"require": {
				"types": "./dist/crypto.d.cts",
				"default": "./dist/crypto.cjs"
			}
		}
	},
	"keywords": [
		"supercell",
		"supercell-api",
		"brawl-stars",
		"clash-royale",
		"clash-of-clans"
	],
	"description": "Supercell ID API for Brawl Stars, Clash Royale, and Clash of Clans",
	"author": "technorav3nn",
	"repository": {
		"type": "git",
		"url": "https://github.com/technorav3nn/brawl-tracker.git",
		"directory": "packages/brawl-stars-api"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
